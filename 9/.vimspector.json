{
  "adapters": {
    "CodeLLDB - StopOnEntry": {
      "extends": "custom-codelldb",
      "name": "CoHostingLLDB"
    }
  },
  "configurations": {
    "cpptools (gdb)": {
      "adapter": "vscode-cpptools",
      "configuration": {
        "request": "launch",
        "program": "${workspaceRoot}/a.out",
        "stopAtEntry": true,
        "MIMode": "gdb"
      }
    }
  }
}

